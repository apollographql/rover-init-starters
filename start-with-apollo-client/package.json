{
  "name": "{{PROJECT_NAME}}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run codegen; tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "grep -q 'schema.*http' codegen.yml && graphql-codegen --config codegen.yml || echo 'No URL schema found or introspection unavailable. Skipping codegen.'",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "postinstall": "npm run codegen || true"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "ai": "^4.3.17",
    "react": "{{REACT_VERSION}}",
    "react-dom": "{{REACT_DOM_VERSION}}",
    "react-router-dom": "{{REACT_ROUTER_DOM_VERSION}}",
    "@apollo/client": "{{APOLLO_CLIENT_VERSION}}",
    "graphql": "{{GRAPHQL_VERSION}}",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "{{TYPES_REACT_VERSION}}",
    "@types/react-dom": "{{TYPES_REACT_DOM_VERSION}}",
    "@typescript-eslint/eslint-plugin": "{{TYPESCRIPT_ESLINT_PLUGIN_VERSION}}",
    "@typescript-eslint/parser": "{{TYPESCRIPT_ESLINT_PARSER_VERSION}}",
    "@vitejs/plugin-react": "{{VITE_PLUGIN_REACT_VERSION}}",
    "eslint": "{{ESLINT_VERSION}}",
    "eslint-plugin-react-hooks": "{{ESLINT_PLUGIN_REACT_HOOKS_VERSION}}",
    "eslint-plugin-react-refresh": "{{ESLINT_PLUGIN_REACT_REFRESH_VERSION}}",
    "typescript": "{{TYPESCRIPT_VERSION}}",
    "vite": "{{VITE_VERSION}}",
    "@graphql-codegen/cli": "{{GRAPHQL_CODEGEN_CLI_VERSION}}",
    "@graphql-codegen/client-preset": "{{GRAPHQL_CODEGEN_CLIENT_PRESET_VERSION}}"
  }
}