{
  "name": "{{PROJECT_NAME}}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run codegen && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "postinstall": "npm run codegen || echo 'Warning: GraphQL code generation failed. This may be due to network issues or invalid schema. You can run npm run codegen manually.'"
  },
  "dependencies": {
    "react": "{{REACT_VERSION}}",
    "react-dom": "{{REACT_DOM_VERSION}}",
    "@apollo/client": "{{APOLLO_CLIENT_VERSION}}",
    "graphql": "{{GRAPHQL_VERSION}}"
  },
  "devDependencies": {
    "@types/react": "{{TYPES_REACT_VERSION}}",
    "@types/react-dom": "{{TYPES_REACT_DOM_VERSION}}",
    "@typescript-eslint/eslint-plugin": "{{TYPESCRIPT_ESLINT_PLUGIN_VERSION}}",
    "@typescript-eslint/parser": "{{TYPESCRIPT_ESLINT_PARSER_VERSION}}",
    "@vitejs/plugin-react": "{{VITE_PLUGIN_REACT_VERSION}}",
    "eslint": "{{ESLINT_VERSION}}",
    "eslint-plugin-react-hooks": "{{ESLINT_PLUGIN_REACT_HOOKS_VERSION}}",
    "eslint-plugin-react-refresh": "{{ESLINT_PLUGIN_REACT_REFRESH_VERSION}}",
    "typescript": "{{TYPESCRIPT_VERSION}}",
    "vite": "{{VITE_VERSION}}",
    "@graphql-codegen/cli": "{{GRAPHQL_CODEGEN_CLI_VERSION}}",
    "@graphql-codegen/typescript": "{{GRAPHQL_CODEGEN_TYPESCRIPT_VERSION}}",
    "@graphql-codegen/typescript-operations": "{{GRAPHQL_CODEGEN_TYPESCRIPT_OPERATIONS_VERSION}}",
    "@graphql-codegen/typescript-react-apollo": "{{GRAPHQL_CODEGEN_TYPESCRIPT_REACT_APOLLO_VERSION}}"
  }
}